{
  "name": "bmatovu/laravel-artisan-gui",
  "description": "Laravel artisan GUI",
  "homepage": "https://github.com/mtvbrianking/laravel-artisan-gui",
  "type": "project",
  "license": "MIT",
  "keywords": [
    "laravel",
    "artisan",
    "gui"
  ],
  "authors": [
    {
      "name": "Brian Matovu",
      "email": "mtvbrianking@gmail.com",
      "homepage": "http://bmatovu.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.2.5|^8.0",
    "illuminate/contracts": "^6.0|^7.0|^8.0",
    "illuminate/http": "^6.0|^7.0|^8.0",
    "illuminate/support": "^6.0|^7.0|^8.0",
    "symfony/console": "^4.3.4|^5.0",
    "symfony/http-kernel": "^4.3.4|^5.0"
  },
  "require-dev": {
    "code-lts/doctum": "^5.3",
    "friendsofphp/php-cs-fixer": "^2.18",
    "orchestra/testbench": "^4.0|^5.0|^6.0",
    "phpunit/phpunit": "^8.0|^9.0"
  },
  "autoload": {
    "psr-4": {
      "Bmatovu\\ArtisanGui\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Bmatovu\\ArtisanGui\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "cs-fix": "php-cs-fixer fix",
    "cs-lint": "php-cs-fixer fix --dry-run",
    "doc": "doctum.php update doctum.php",
    "test": "phpunit",
    "test-coverage": "phpunit --coverage-html coverage"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Bmatovu\\ArtisanGui\\ArtisanGuiServiceProvider"
      ]
    }
  }
}
